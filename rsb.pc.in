prefix=@CMAKE_INSTALL_PREFIX@
libdir=${prefix}/lib
includedir=${prefix}/include/@INSTALL_PATH_PREFIX@

Name: Robotics Systems Bus
Description: Fully event-driven Robotics Systems Bus
Version: @RSB_VERSION@
Requires: rsc@VERSION_SUFFIX@
